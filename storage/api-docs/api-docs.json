        "/oauth/authorize?client_id&redirect_uri&response_type&scope&state": {
            "get": {
                "tags": [
                    "OAuth2"
                ],
                "parameters": [
                    {
                        "name": "client_id",
                        "in": "query",
                        "example": "1",
                        "description": "Client ID",
                        "type": "string",
                        "required": true
                    },
                    {
                        "name": "redirect_uri",
                        "in": "query",
                        "example": "http://localhost/dashboard",
                        "description": "Callback / Client redirect URL",
                        "type": "string",
                        "required": true
                    },
                    {
                        "name": "response_type",
                        "in": "query",
                        "description": "code / This tells the authorization server that the application is initiating the authorization code flow",
                        "type": "string",
                        "example": "code",
                        "required": true
                    },
                    {
                        "name": "state",
                        "in": "query",
                        "description": "The application generates a random string and includes it in the request. It should then check that the same value is returned after the user authorizes the app. This is used to prevent CSRF attacks.",
                        "type": "string",
                        "example": "5t5trgfddfUDRGhthrt",
                        "required": true
                    },
                    {
                        "name": "scope",
                        "in": "query",
                        "description": "One or more space-separated strings indicating which permissions the application is requesting. The specific OAuth API youâ€™re using will define the scopes that it supports.",
                        "type": "string"
                    }
                ],
                "summary": "OAuth API",
                "operationId": "ec2a3099247gtreba480e78b8d7337",
                "responses": {
                    "200": {
                        "description": "success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "code": {
                                            "type": "string",
                                            "example": ""
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
